@model ErrorDTO
    <!-- Start Error-Area Area  -->
<div class="error-area ptb--200 ptb_sm--60 ptb_md--80">
    <div class="container">
        <div class="row align-item-center">
            <div class="col-lg-12">
                <div class="error-inner" style="position: relative;">

                    <div class="bar"></div>
                    <iframe class="frame" src='https://my.spline.design/untitled-eb871cfd85566910b62f0a63df48712f/' frameborder='0' style="width: 100%; height:400px;"></iframe>
                </div>
                <div class="error-inner">

                    <h2 class="title">Something’s not right.</h2>
                    <p class="description">
                        @Model.Message
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
